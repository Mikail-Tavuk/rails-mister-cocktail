<div id="container">
  <div id="row">
    <div id="column">
      <h1>Cocktail's list🍸</h1>
      <ul>
      <% @cocktails.each do |cocktail| %>
        <li><%= link_to cocktail.name, cocktail_path(cocktail) %></li>
      <% end %>
      </ul>
      <%= link_to "+ Want to add a new cocktail ?", new_cocktail_path %>
      
    </div>
  </div>
</div>